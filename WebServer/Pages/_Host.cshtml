@page "/"
@using Microsoft.AspNetCore.Components.Web
@namespace WebServer.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<base href="~/" />
	<link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
	<link href="css/site.css" rel="stylesheet" />
	<link href="WebServer.styles.css" rel="stylesheet" />
	<link rel="icon" type="image/png" href="favicon.png" />
	<component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />

	<!-- Custom fonts for this template-->
	<link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
	<link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
		  rel="stylesheet">

	<!-- Custom styles for this template-->
	<link href="~/css/sb-admin-2.min.css" rel="stylesheet">

	<link href="_content/Syncfusion.Blazor.Themes/bootstrap5.css" rel="stylesheet" />

	<script src="https://kit.fontawesome.com/a7e27089bf.js" crossorigin="anonymous"></script>
</head>
<body id="page-top">
	<component type="typeof(App)" render-mode="ServerPrerendered" />

	<div id="blazor-error-ui">
		<environment include="Staging,Production">
			An error has occurred. This application may no longer respond until reloaded.
		</environment>
		<environment include="Development">
			An unhandled exception has occurred. See browser dev tools for details.
		</environment>
		<a href="" class="reload">Reload</a>
		<a class="dismiss">🗙</a>
	</div>

	<div id="components-reconnect-modal"></div>


	<script src="_framework/blazor.server.js"></script>

	<!-- Bootstrap core JavaScript-->
	<script src="~/vendor/jquery/jquery.min.js"></script>
	<script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

	<!-- Core plugin JavaScript-->
	<script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>

	<!-- Custom scripts for all pages-->
	<script src="~/sb-admin-2.min.js"></script>

	<script src="_content/Syncfusion.Blazor.Core/scripts/syncfusion-blazor.min.js" type="text/javascript"></script>

	<script src="https://cdnjs.cloudflare.com/ajax/libs/paho-mqtt/1.0.2/mqttws31.min.js" integrity="sha512-Gch3eVsdc+qexpSPzF9XxYV6aTfrBOSazZiPhNx2Xt+xIrPOTE6TCDXduwqMb8f+L8kDfjwdn8rabNHfaGMtrA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

	<script src="https://cdn.jsdelivr.net/npm/@@tensorflow/tfjs-core"></script>
	<script src="https://cdn.jsdelivr.net/npm/@@tensorflow/tfjs-backend-cpu"></script>
	<script src="https://cdn.jsdelivr.net/npm/@@tensorflow/tfjs-tflite@@0.0.1-alpha.9/dist/tf-tflite.min.js"></script>

	<script src="~/common.js"></script>
	<script src="~/video.js"></script>
	<script src="~/signalr.js"></script>
	<script src="~/rtc.js"></script>
	<script src="~/bluetooth/bluetooth.js"></script>
	<script src="~/mqtt/send.js"></script>

	<script src="~/tflite/load_tflite.js"></script>
	<script src="~/tflite/process.js"></script>
	<script src="~/tflite/predict.js"></script>
	<script src="~/tflite/motion.js"></script>
	<script src="~/tflite/pose.js"></script>
	<script src="~/tflite/blur.js"></script>
</body>
</html>
